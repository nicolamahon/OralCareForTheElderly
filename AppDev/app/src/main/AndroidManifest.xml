<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projects.nicola.oralcarefortheelderly">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NaturalTeeth"
            android:label="@string/title_activity_natural_teeth"
            android:parentActivityName=".self_care" />
        <activity
            android:name=".cavities"
            android:parentActivityName=".NaturalTeeth" />
        <activity
            android:name=".MainActivity"
            android:label="Main Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".self_care"
            android:label="Self Care Menu"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".patient_care" />
        <activity android:name=".information" />
        <activity android:name=".login"></activity>
    </application>

</manifest>